{
  "name": "arch-v-server",
  "version": "0.0.1",
  "main": "lambda.js",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0-0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "claudia": "^5.1.1",
    "nodemon": "^1.18.4"
  },
  "scripts": {
    "dev": "nodemon ./src/index.js --exec babel-node",
    "clean": "rm -rf build && mkdir build && rm lambda.js",
    "transpile": "babel -d ./build ./src -s",
    "build": "npm run clean && npm run transpile && claudia generate-serverless-express-proxy --express-module build/index",
    "create": "rm -rf node_modules && npm install && npm run-script build",
    "deploy": "claudia create --handler lambda.handler --deploy-proxy-api --region us-west-2 --use-s3-bucket arch-v --keep",
    "start": "node ./build/index.js"
  },
  "dependencies": {
    "@tensorflow/tfjs-node": "^0.1.17",
    "aws-sdk": "^2.325.0",
    "aws-serverless-express": "^3.3.5",
    "canvas": "^2.0.0-alpha.13",
    "dotenv-safe": "^6.0.0",
    "express": "^4.16.3",
    "helmet": "^3.13.0",
    "image-size": "^0.6.3",
    "multer": "^1.3.1",
    "multer-s3": "^2.7.0",
    "node-fetch": "^2.2.0"
  },
  "engines": {
    "yarn": "YARN NO LONGER USED - use npm instead."
  },
  "files": [
    "build",
    "public",
    ".env",
    ".env.example"
  ]
}
