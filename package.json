{
  "name": "arch-v-server",
  "version": "0.0.1",
  "main": "lambda.js",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0-0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "claudia": "^5.1.1",
    "nodemon": "^1.18.4"
  },
  "scripts": {
    "dev": "nodemon ./src/index.js --exec babel-node",
    "clean": "rm -rf build && mkdir build && rm lambda.js",
    "build-server": "babel -d ./build ./src -s",
    "build": "npm run clean && npm run build-server && claudia generate-serverless-express-proxy --express-module build/index",
    "create": "claudia create --handler lambda.handler --deploy-proxy-api --region us-west-2",
    "deploy": "rm -rf node_modules && npm run build && npm run create",
    "start": "node ./build/index.js"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^0.13.0",
    "@tensorflow/tfjs-converter": "^0.6.0",
    "@tensorflow/tfjs-node": "^0.1.17",
    "canvas": "^1.6.12",
    "dotenv-safe": "^6.0.0",
    "express": "^4.16.3",
    "helmet": "^3.13.0",
    "image-size": "^0.6.3",
    "multer": "^1.3.1",
    "node-fetch": "^2.2.0"
  },
  "engines": {
    "yarn": "YARN NO LONGER USED - use npm instead."
  },
  "files": [
    "build"
  ]
}
